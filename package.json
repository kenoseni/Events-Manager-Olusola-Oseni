{
  "name": "events-manager-olusola-oseni",
  "version": "1.0.0",
  "description": "An app that enables you manage event centers",
  "main": "app.js",
  "engines": {
    "node": "7.7.4"
  },
  "scripts": {
    "start": "babel-node ./server/app.js",
    "dev": "./node_modules/.bin/webpack-dev-server --config ./webpack.config.js",
    "start:dev": "nodemon ./server/app --exec babel-node",
    "test": "nyc mocha --timeout 250000 tests/test.js --require babel-register --exit",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "nyc": "nyc report",
    "compile-component": "babel client/components --presets es2015 --out-dir client/public --source-maps",
    "watch": "webpack -d && webpack-dev-server --hot --inline",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "unmigrate": "node_modules/.bin/sequelize db:migrate:undo:all"
  },
  "keywords": [
    "API"
  ],
  "author": "Olusola Oseni",
  "license": "ISC",
  "dependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.1.0",
    "mocha-lcov-reporter": "^1.3.0",
    "node-sass": "^4.7.2",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "sass-loader": "^6.0.6",
    "sequelize": "^4.22.8",
    "sequelize-cli": "^3.1.0",
    "volleyball": "^1.4.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.0",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "supertest": "^3.0.0"
  }
}
