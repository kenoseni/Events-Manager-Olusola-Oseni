{
  "name": "events-manager-olusola-oseni",
  "version": "1.0.0",
  "description": "An app that enables you manage event centers",
  "main": "app.js",
  "engines": {
    "node": "7.7.4",
    "npm": "5.4.2"
  },
  "scripts": {
    "start": "npm run clean && npm run build && npm run compile && NODE_ENV=production node ./lib/app.js",
    "dev": "./node_modules/.bin/webpack-dev-server --config ./webpack.config.js",
    "start:dev": "nodemon ./server/app --exec babel-node",
    "test": "nyc --reporter=html --reporter=lcov --reporter=text mocha --timeout 250000 tests/test.js --require babel-register --exit",
    "test:client": "jest",
    "test:client:watch": "npm run test:client --watch",
    "build": "babel server -d lib --source-maps inline --copy-files",
    "heroku-postbuild": "npm run build",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "nyc": "nyc report",
    "compile-component": "babel client/components --presets es2015 --out-dir client/public --source-maps",
    "watch": "webpack -d && webpack-dev-server --hot --inline",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "unmigrate": "node_modules/.bin/sequelize db:migrate:undo:all",
    "seed": "node_modules/.bin/sequelize db:seed:all",
    "unseed": "node_modules/.bin/sequelize db:seed:undo:all",
    "compile": "NODE_ENV=production webpack --config ./webpack.config.prod.js",
    "clean": "rm -rf ./client/dist/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kenoseni/Events-Manager-Olusola-Oseni.git"
  },
  "keywords": [
    "API"
  ],
  "author": "Olusola Oseni",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.17.1",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "coveralls": "^3.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "file-loader": "^1.1.5",
    "history": "^4.7.2",
    "image-webpack-loader": "^3.4.2",
    "istanbul": "^0.4.5",
    "istanbul-coveralls": "^1.0.3",
    "jsonwebtoken": "^8.1.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.10",
    "node-sass": "^4.7.2",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "sequelize": "^4.22.8",
    "sequelize-cli": "^3.1.0",
    "shortid": "^2.2.8",
    "style-loader": "^0.19.0",
    "swagger-ui-express": "^3.0.9",
    "toastr": "^2.1.4",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "volleyball": "^1.4.1",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-dev-server": "^2.9.4",
    "webpack-hot-middleware": "^2.21.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.1",
    "chai": "^4.1.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.0",
    "expect": "^22.4.3",
    "jest": "^22.4.2",
    "jquery": "^3.3.1",
    "mocha": "^4.0.1",
    "moxios": "^0.4.0",
    "nyc": "^11.3.0",
    "react-test-renderer": "^16.4.1",
    "redux-mock-store": "^1.5.1",
    "sinon": "^6.1.3",
    "supertest": "^3.0.0"
  },
  "jest": {
    "verbose": true,
    "rootDir": "client",
    "roots": [
      "<rootDir>"
    ],
    "setupFiles": [],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/__tests__/__mocks__/",
      "/template/",
      "/sever/",
      "/build/",
      "/.github/",
      "/.nyc_output/",
      "/coverage/",
      "/lib/",
      "<rootDir>/coverage/",
      "<rootDir>/public"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/coverage/",
      "<rootDir>/public"
    ],
    "unmockedModulePathPatterns": [
      "./node_modules/react"
    ],
    "testEnvironment": "jsdom",
    "snapshotSerializers": [],
    "moduleFileExtensions": [
      "js"
    ],
    "collectCoverage": true,
    "moduleNameMapper": {}
  }
}
